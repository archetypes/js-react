/** @jsx React.DOM */

var rewire = require('rewire');
var ReactTest = require('./ReactTest');
var props = ReactTest.mock.props;
var expect = require('chai').expect;
var {{project}} = rewire('../../main/js/{{project}}');

ReactTest.mock({{project}}, 'Label');

describe('{{project}}', function() {
  it('shows a label', function() {
    var $ = ReactTest.render(<{{project}}/>);
    expect(props($('#Label')).children).to.eql('{{project}}');
  });
});